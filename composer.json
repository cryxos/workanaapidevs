{
    "require": {
        "php-di/php-di": "^6.4",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "tuupola/slim-jwt-auth": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
